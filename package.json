{
  "name": "@okanjo/okanjo-sdk",
  "version": "0.4.9",
  "description": "Use the Okanjo API in JavaScript!",
  "main": "lib/client.js",
  "scripts": {
    "test": "node_modules/.bin/mocha test",
    "cover": "rm -f coverage/coverage*.json && node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- -R spec && node_modules/.bin/istanbul report",
    "hint": "node_modules/.bin/jshint ./lib/**/*.js",
    "report": "npm run cover && npm run hint",
    "browser": "./node_modules/.bin/browserify -i ./lib/providers/http_provider.js -r ./lib/client.js:okanjo  -o dist/api.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Okanjo/okanjo-nodejs.git"
  },
  "keywords": [
    "Okanjo",
    "API",
    "Ecommerce",
    "Marketplace",
    "Ads",
    "AdCommerce",
    "ProductSense"
  ],
  "author": "Okanjo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Okanjo/okanjo-nodejs/issues"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "mocha": "^2.4.1",
    "should": "^8.2.0"
  }
}

{
  "name": "okanjo",
  "version": "2.5.0",
  "description": "Integrate your application with the Okanjo API.",
  "main": "dist/client.js",
  "scripts": {
    "build": "node_modules/.bin/gulp build",
    "clean": "rm -rf .nyc_output coverage",
    "test": "npm run build && node_modules/.bin/mocha -R spec test",
    "cover": "node_modules/.bin/nyc node_modules/mocha/bin/_mocha --exit",
    "lint": "node_modules/.bin/eslint .",
    "report": "npm run cover && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Okanjo/okanjo-nodejs.git"
  },
  "keywords": [
    "Okanjo",
    "API"
  ],
  "author": "Okanjo Partners Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Okanjo/okanjo-nodejs/issues"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-nunjucks-render": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "eslint": "^4.10.0",
    "mocha": "^4.0.1",
    "needle": "^1.1.2",
    "node-fetch": "^1.6.3",
    "should": "^13.1.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "nyc": "^11.3.0"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html",
      "lcovonly"
    ]
  }
}
